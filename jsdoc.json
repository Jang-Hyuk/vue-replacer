{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./temp/utils.js", "./temp/util.qs.js", "./temp/util.device.js", "./temp/util.str.js", "./temp/util.hardware.js"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs|doc)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "doc/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}