    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "prettier": "^2.5.1",
    "taffydb": "^2.7.3",
    "vue": "^2.3.12"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "iconv-lite": "^0.6.3",
