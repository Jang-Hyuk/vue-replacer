{
  "name": "vue-replacer",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "jsdoc": "nodemon  --trace-warnings source/index.js",
    "docs": "typedoc --searchInComments --name \"Club5678 Procedure API\" --tsconfig ../tsconfig.typedoc.json",
    "dev": "nodemon --trace-warnings src/index.js",
    "dev-d": "nodemon --trace-warnings src/index.js dec",
    "dev-e": "nodemon --trace-warnings src/index.js enc",
    "dev-de": "nodemon --trace-warnings src/index.js dec-enc",
    "dev:ie": "nodemon --trace-warnings src/index.js ie",
    "dev-encode": "nodemon --trace-warnings src/runEncode.js",
    "dev-encode:ie": "nodemon --trace-warnings src/runEncode.js ie",
    "decode": "node --trace-warnings src/tests/decode.js",
    "main": "node src/tests/replacer.js",
    "test-main": "nodemon --trace-warnings src/tests/replacer.js",
    "test-manage": "nodemon --trace-warnings src/tests/fileManage.js",
    "test-encode": "nodemon  --trace-warnings src/tests/encode.js",
    "test-encode:ie": "nodemon  --trace-warnings src/tests/encode.js ie",
    "test-decode": "nodemon  --trace-warnings src/tests/decode.js",
    "test-decode:ie": "nodemon  --trace-warnings src/tests/decode.js ie",
    "test-jsdoc": "nodemon  --trace-warnings source/tests/encodeJsdoc.js"
  },
  "keywords": [],
  "author": "",
