{
  "name": "vue-replacer",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "convert": "nodemon  --trace-warnings source/index.js",
    "jsdoc": "yarn convert docs",
    "docs-club": "typedoc --tsconfig ../tsconfig.typedoc.json",
    "docs-work": "typedoc --tsconfig ./tsconfig.typedoc.json",
    "docs": "yarn docs-club && yarn docs-work",
    "dev": "nodemon --trace-warnings src/index.js",
    "dev-d": "nodemon --trace-warnings src/index.js dec",
    "dev-e": "nodemon --trace-warnings src/index.js enc",
    "dev-de": "nodemon --trace-warnings src/index.js dec-enc",
    "dev:ie": "nodemon --trace-warnings src/index.js ie",
    "test-jsdoc": "nodemon  --trace-warnings source/tests/encodeJsdoc.js",
    "test-util": "nodemon  --trace-warnings source/tests/convertUtil.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "docdash": "^2.0.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "jsdoc": "^4.0.2",
    "prettier": "^2.5.1",
    "taffydb": "^2.7.3",
    "typedoc": "^0.23.25",
    "typedoc-theme-hierarchy": "^3.0.2",
    "typescript": "^4.9.5",
    "vue": "^2.3.12"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "node-watch": "^0.7.3",
    "uuid": "^8.3.2"
  }
}
